    </frames>
    <objects>
      <object shape="rectangle" type="formula" name="BOX" color="light red" />
    </objects>
    <expr>
		BOX.left   = A.left + 1;
		BOX.top    = A.top + 1;

        LMO = A.left_margin;
		TMO = A.top_margin;

        BOX.right  = A.right - 1 + (A.left_margin - LMO);
        BOX.bottom = A.bottom - 1 + (A.top_margin - TMO);

        A.left_margin = 3;
        A.top_margin = 3;
        A.right_margin = 3;
        A.bottom_margin = 3;
