		<point type="lineto" x =   "92" 	y = "100" />
		<point type="lineto" x =   "92" 	y = "0" />
      </object>

      <object name="ASE" 
               shape="spline" type="formula"
              alt-group="2"  >
		<point type="moveto" x =  "0	 	"y = "101,666666667		"/>
		<point type="splnto" x =  "0	 	"y = "102,5	"/>
		<point type="splnto" x =  "40	 	"y = "91,666666667"/>
		<point type="splnto" x =  "65	 	"y = "70	"/>
		<point type="splnto" x =  "96,66667	"y = "41,66666667"/>
		<point type="splnto" x =  "100	 	"y = "15	"/>
		<point type="splnto" x =  "100	 	"y = "0	"/>
		<point type="lineto" x =  "92	 	"y = "0	"/>
		<point type="splnto" x =  "92	 	"y = "28,33333333	"/>
		<point type="splnto" x =  "67	 	"y = "79	"/>
		<point type="splnto" x =  "0	 	"y = "100	"/>
      </object> 

	   <object name="CNE" 
              shape="spline" type="formula"
              alt-group="2"  >
		<point type="moveto" x =   "-8	 			"y = "0	"/>
		<point type="lineto" x =   "0			 	"y = "0	"/>
		<point type="splnto" x =  "2				"y = "40	"/>
		<point type="splnto" x =  "50	 			"y = "80	"/>
		<point type="splnto" x =  "78				"y = "92	"/>

		<point type="lineto" x =  "78	 			"y = "97	"/>
		<point type="splnto" x =  "63	 			"y = "90	"/>
		<point type="splnto" x =  "-12	 			"y = "50	"/>
		<point type="splnto" x =  "-8	 			"y = "-1	"/>	

      </object>

      <object name="CSE" 
              shape="spline" type="formula"
              alt-group="2"  >
		<point type="moveto" x =   "-8	 	"y = "100	"/>
		<point type="lineto" x =   "0	 	"y = "100	"/>
		<point type="splnto" x =  "2	 	"y = "60	"/>
		<point type="splnto" x =  "50	 	"y = "20	"/>
		<point type="splnto" x =  "78		"y = "8	"/>
		
		<point type="lineto" x =  "78	 	"y = "3	"/>
		<point type="splnto" x =  "63	 	"y = "10	"/>
		<point type="splnto" x =  "-12	 	"y = "50	"/>
		<point type="splnto" x =  "-8	 	"y = "101	"/>	
      </object>

    </objects>
    <expr>
		DX = 4;
		DY = 4;
		x_margin = 1;
		y_margin = 0.25;
		
        C = (bottom + top) / 2;

        L1HI.top    = top + DY;
        L1HI.bottom = C - DY;
        L1HI.right  = right - DX - 1;
        L1HI.left   = L1HI.right - DX;

        L1LO.top    = C + DY;
        L1LO.bottom = bottom - DY;
        L1LO.left   = L1HI.left;
        L1LO.right  = L1HI.left + DX;

		ANE.top    = top + y_margin;
		ANE.bottom = L1HI.top;
		ANE.left   = L1HI.left;
		ANE.right  = L1HI.right;

		ASE.top    = L1LO.bottom;
		ASE.bottom = bottom - y_margin;
		ASE.left   = ANE.left;
		ASE.right  = ANE.right;

		CNE.top    = L1HI.bottom;
		CNE.bottom = L1HI.bottom + DY;
		CNE.left   = L1HI.right/* - 1*/;
		CNE.right  = CNE.left + DX;

		CSE.top    = CNE.bottom;
		CSE.bottom = L1LO.top;
		CSE.left   = CNE.left;
		CSE.right  = CNE.right;
    </expr>
    <mathml type="mo" value="}" />
    <dsr head="}" param="$empty$" />
