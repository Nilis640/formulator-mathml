<!-- TOOLBAR: Hidden templates                                              -->
<!--                                                                        -->
<toolbar id="@t@Hidden@"
         group="Presentation"
         name="Hidden templates" 
         caption="Hidden"
		 visible="no" 
		         image="" >
        <!-- image="images/buttons/hidden.bmp" >-->

  <!-- Box (Ctrl+Shift+T,X)                                                 -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Box (Ctrl+Shift+T,X)" id="@box_dash" 
          vertical-center-type="frame" vertical-center-value="1"
  >
    <hot-keys>
      <hot-key>
        <sequence ctrl="yes" shift="yes" key="T" />
        <sequence                        key="X" />
      </hot-key>
    </hot-keys>
    <frames>
      <frame name="A" left="0" top="0" right="100" bottom="100" halign="center" valign="center" />
    </frames>
    <objects>
      <object shape="rectangle" pen-style="dash" type="formula" name="BOX" />
    </objects>
    <expr>
		BOX.left   = A.left + 1;
		BOX.top    = A.top + 1;

        LMO = A.left_margin;
		TMO = A.top_margin;

        BOX.right  = A.right - 1 + (A.left_margin - LMO);
        BOX.bottom = A.bottom - 1 + (A.top_margin - TMO);

        A.left_margin = 3;
        A.top_margin = 3;
        A.right_margin = 3;
        A.bottom_margin = 3;
    </expr>
    <mathml name="menclose" type="box-dashed" />
    <dsr head="@box( " tail=")" separator="," param="$child_list$" bracket="off" />
  </button>


  <!-- Division operator                                                    -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Division operator" type="text" value="/" style="op" id="text /"
    stretchy="yes"
  >
    <objects>
      <object shape="line" name="L1" type="formula" alt-group="2" />
    </objects>
    <expr>
		DY = (bottom - top) * 0.8;
		DX = DY / 3.0;
		LR = (0.1666670 * em);
		TB = (bottom - top - DY) / 2;

		right = DX + 2 + LR + LR;
        L1.bottom = top + TB;
        L1.top    = L1.bottom + DY;
        L1.left   = left + 1 + LR;
        L1.right  = L1.left + DX;

    </expr>
    <mathml type="mo" value="/" />
    <dsr head="/" param="$empty$" />
  </button>


  <!-- Under bar                                                            -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Under bar" type="text" value="_" style="op" id="text _"
    stretchy="yes"
  >
    <objects>
      <object shape="line" name="L" type="formula" />
    </objects>
    <expr>
        L.left   = left;
        L.right  = right;
        L.top    = bottom;
        L.bottom = bottom;
    </expr>
    <mathml value="&UnderBar;" type="mo" />
    <dsr head="_" param="$empty$" />
  </button>


