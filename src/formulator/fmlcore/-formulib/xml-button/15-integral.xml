<!-- TOOLBAR: Integral templates                                            -->
<!--                                                                        -->
<toolbar id="@t@Integral@"
         group="Presentation"
         name="Integral templates" 
         caption="Integral"
         image="images/buttons/integral.bmp"
         image-title="images/title/t_integral_n.bmp" 
         image-title-hot="images/title/t_integral_h.bmp" >

  <!-- Indefinite integral (no limits)                                      -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Indefinite integral (no limits) (Ctrl+Shift+I,!)" 
          vertical-center-type="frame" vertical-center-value="1"
          id="@integral_nl" type="mathml" >
    <hot-keys>
      <hot-key>
        <sequence ctrl ="yes" shift="yes" key="I" />
        <sequence             shift="yes" key="!" />
      </hot-key>
    </hot-keys>
    <math display='block'>
      <mrow>
        <mo>&int;</mo>
        <mrow></mrow>
      </mrow>
    </math>
  </button>


  <!-- Definite integral with underscript and overscript limits             -->
