<!-- TOOLBAR: Products and set theory templates                             -->
<!--                                                                        -->
<toolbar id="@t@Products@"
         group="Presentation"
         name="Products and set theory templates" 
         caption="Products"
         image="images/buttons/products.bmp"
         image-title="images/title/t_products_n.bmp" 
         image-title-hot="images/title/t_products_h.bmp" >


  <!-- Product with no limits                                               -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Product with no limits (Ctrl+T,Shift+P)" 
          vertical-center-type="frame" vertical-center-value="1"
          id="@product_nl" type="mathml" >
    <hot-keys>
      <hot-key>
        <sequence ctrl ="yes" key="T" />
        <sequence shift="yes" key="P" />
      </hot-key>
    </hot-keys>
    <math display='block'>
      <mrow>
        <mo largeop='true'>&prod;</mo>
        <mrow></mrow>
      </mrow>
    </math>
  </button>


  <!-- Product with underscript limit                                       -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
