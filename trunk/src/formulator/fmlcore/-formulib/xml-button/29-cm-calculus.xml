<!-- TOOLBAR: Content MathML: Calculus and Vector Calculus Templates        -->
<!--                                                                        -->
<toolbar id="@t@CMCalculus@"
         group="Content"
         name="Calculus and vector calculus"
         caption="Calculus" 
         image="images/buttons/cm_calculus.bmp"
         image-title="images/title/t_cm_calculus_n.bmp"
         image-title-hot="images/title/t_cm_calculus_h.bmp"

         columns="5" >


  <!-- int                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The int element is the operator element for an integral"
          id="@cm_int" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
          <int/>
    </math>
  </button>


  <!-- int                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The int element is the operator element for an integral"
          id="@cm_int1" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <apply>
          <int/>
          <mrow __flag="0x20000000"></mrow>
        </apply>
    </math>
  </button>


  <!-- int                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The int element is the operator element for an integral (a definite integral, using the qualifiers lowlimit, uplimit)"
          id="@cm_int2" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <apply>
          <int/>
          <bvar><mrow __flag="0x20000000"></mrow></bvar>
          <lowlimit><mrow __flag="0x20000000"></mrow></lowlimit>
          <uplimit><mrow __flag="0x20000000"></mrow></uplimit>
          <mrow __flag="0x20000000"></mrow>
        </apply>
    </math>
  </button>


  <!-- int                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The int element is the operator element for an integral (specifies the domain of integration with an interval element)"
          id="@cm_int3" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
