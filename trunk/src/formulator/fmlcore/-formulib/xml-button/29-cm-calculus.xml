        <apply>
          <int/>
          <bvar><mrow __flag="0x20000000"></mrow></bvar>
          <interval>
            <mrow __flag="0x20000000"></mrow>
            <mrow __flag="0x20000000"></mrow>
          </interval>
          <mrow __flag="0x20000000"></mrow>
        </apply>
    </math>
  </button>


  <!-- int                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The int element is the operator element for an integral (specifies the domain of integration with an condition element)"
