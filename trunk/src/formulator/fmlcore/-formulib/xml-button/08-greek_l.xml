<!--                                                                        -->
<!-- TOOLBAR: Greek characters (lowercase)                                  -->
<!--                                                                        -->
<toolbar id="@t@GreekLo@"
         group="Presentation"
         name="Greek characters (lowercase)" 
         caption="Greek Lo" 
         image="images/buttons/greek_l.bmp" 
         image-title="images/title/t_greek_l_n.bmp" 
         image-title-hot="images/title/t_greek_l_h.bmp" >

  <!-- Greek characters (lowercase)                                         -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Greek small letter alpha (Ctrl+G,A)"
          type="text" value="&amp;#x03B1;" style="mathextra" id="alpha" >
    <hot-keys>
      <hot-key>
        <sequence ctrl="yes" key="G" />
        <sequence key="A" />
      </hot-key>
    </hot-keys>
    <mathml value="&alpha;" type="mi" />
    <dsr head="" tail="$g " separator="" param="$child_list$" bracket="off" />
  </button>

