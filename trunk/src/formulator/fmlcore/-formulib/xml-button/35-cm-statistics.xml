<!-- TOOLBAR: Content MathML: Statistics Templates                          -->
<!--                                                                        -->
<toolbar id="@t@CMStatistics@"
         group="Content"
         name="Statistics"
         caption="Statistics" 
         image="images/buttons/cm_statistics.bmp"
         image-title="images/title/t_cm_statistics_n.bmp"
         image-title-hot="images/title/t_cm_statistics_h.bmp"
         columns="5" >


  <!-- mean                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="mean is the operator element representing a mean or average"
          id="@cm_mean" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
          <mean/>
    </math>
  </button>


  <!-- mean                                                                -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="mean is the operator element representing a mean or average"
          id="@cm_mean1" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <apply>
          <mean/>
          <mrow __flag="0x20000000"></mrow>
        </apply>
    </math>
  </button>


